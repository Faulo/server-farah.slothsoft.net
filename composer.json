{
    "name": "slothsoft/farah.slothsoft.net",
    "type": "project",
    "description": "farah.slothsoft.net",
    "license": "GPL",
    "keywords": [
        "slothsoft",
        "farah-module",
        "farah-server"
    ],
    "autoload": {
        "files": [
            "config.php",
            "src/registerModule.php"
        ]
    },
    "require": {
        "php": "^7.2",
        "slothsoft/core": "dev-master",
        "slothsoft/farah": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": true,
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": "Slothsoft\\Core\\ServerEnvironment::cleanCacheDirectory"
    }
}
